<body>
    <header class="main-header">
        <%- include("includes/nav.ejs") %>
    </header>

    <main>
        <h1>My Products</h1>
        <% if (hasProducts) { %>
            <div>
                <% for(let product of prods){ %>
                    <p><%= product.title %></p>
                 <% } %>   
            </div>
        
       <% }else{ %>
        <p>No products found</p>
       <% } %>
        
    </main>
</body>

</html>